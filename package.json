{
  "name": "cardinal",
  "version": "1.0.0",
  "description": "A Discord bot",
  "main": "build/index.js",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "lint": "eslint . --ext .ts,.js,.json",
    "lint:fix": "eslint . --ext .ts,.js,.json --fix",
    "format": "prettier --config .prettierrc '**/*.{json,js,ts,md}' --write",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itshaydendev/cardinal.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Hayden Young <hayden@itshayden.dev> (https://itshayden.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itshaydendev/cardinal/issues"
  },
  "homepage": "https://github.com/itshaydendev/cardinal#readme",
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/turndown": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@sentry/node": "^5.19.2",
    "chalk": "^4.1.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "pg": "^8.5.1",
    "turndown": "^6.0.0",
    "typeorm": "^0.2.29"
  }
}
